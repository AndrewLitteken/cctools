{
  "rules":[
    {
      "command":"echo A > A",
      "outputs":["A"]
    },
    {
      "command":"echo B > B",
      "outputs":["B"]
    },
    {
      "command":"echo C > C && cat A B >> C",
      "outputs":["C"],
      "inputs":["A", "B"]
    },
    {
      "command":"sort C > D",
      "outputs":["D"],
      "inputs":["C"]
    },
    {
      "command": "[ $(cat C D | md5sum | cut -d ' ' -f 1) = 0d0d98ed89144d056c149efc75b0b2cc ]",
      "inputs": ["C", "D"]
    }
  ]
}
