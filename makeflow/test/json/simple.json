{
  "rules":[
    {
      "command":"echo A > A",
      "outputs":[{"path": "A"}]
    },
    {
      "command":"echo B > B",
      "outputs":[{"path": "B"}]
    },
    {
      "command":"echo C > C && cat A B >> C",
      "outputs":[{"path": "C"}],
      "inputs":[{"path": "A"}, {"path": "B"}]
    },
    {
      "command":"sort C > D",
      "outputs":[{"path": "D"}],
      "inputs":[{"path": "C"}]
    },
    {
      "command": "[ $(cat C D | md5sum | cut -d ' ' -f 1) = 0d0d98ed89144d056c149efc75b0b2cc ]",
      "inputs": [{"path": "C"}, {"path": "D"}]
    }
  ]
}
