{
  "rules":[
    {
      "command":"echo A > A",
      "outputs":[{"name_on_submission": "A"}]
    },
    {
      "command":"echo B > B",
      "outputs":[{"name_on_submission": "B"}]
    },
    {
      "command":"echo C > C && cat A B >> C",
      "outputs":[{"name_on_submission": "C"}],
      "inputs":[{"name_on_submission": "A"}, {"name_on_submission": "B"}]
    },
    {
      "command":"sort C > D",
      "outputs":[{"name_on_submission": "D"}],
      "inputs":[{"name_on_submission": "C"}]
    },
    {
      "command": "[ -e C ] && [ -e D ]",
      "inputs": [{"name_on_submission": "C"}, {"name_on_submission": "D"}]
    }
  ]
}
