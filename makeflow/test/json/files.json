{
  "rules":[
    {
      "command":"echo A > A",
      "outputs":[{"name_on_submission": "A"}]
    },
    {
      "command":"cat A > B && echo B >> B",
      "inputs": [{"name_on_submission": "A"}],
      "outputs":[{"name_on_submission": "B"}]
    },
    {
      "inputs": [{"name_on_submission": "A"}, {"name_on_submission": "B"}],
      "command": "wc -l A | grep 1 && wc -l B | grep 2"
    }
  ]
}
