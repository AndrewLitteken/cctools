{
  "rules":[
    {
      "command":"echo A > A",
      "outputs":[{"path": "A"}]
    },
    {
      "command":"cat A > B && echo B >> B",
      "inputs": [{"path": "A"}],
      "outputs":[{"path": "B"}]
    },
    {
      "inputs": [{"path": "A"}, {"path": "B"}],
      "command": "[ \"$(wc -l A)\" = '1 A' ] && [ \"$(wc -l B)\" = '2 B' ]"
    }
  ]
}
