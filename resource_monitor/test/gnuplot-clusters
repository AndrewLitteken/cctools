reset
#unset key; 
set xtics nomirror; set ytics nomirror; set border front;
div=1.1; bw = 0.9; h=1.0; BW=0.9; wd=10; LIMIT=255-wd; white = 0 
red = "#080000"; green = "#000800"; blue = "#000008";
set terminal jpeg size 1024,768
set auto x
set auto y
set style data histogram
set style histogram rowstacked
set style fill solid
set boxwidth bw
set multiplot
call 'clusters_plot_cmd'
unset border; set xtics format " "; set ytics format " "; set ylabel " "
unset multiplot
